{% extends('BooksmartMetricsSoundcloudBundle:Default:base.html.twig') %}

{%block body%}

<table class = 'table table-bordered'>
	<thead>
		<tr>
			<th>
				ID
			</th>
			<th>
				Name
			<th>
		</tr>
	</thead>
	<tbody>
{% for artist in artists %}
		<tr>
			<td>
				{{artist.id}}
			</td>
			<td>
				{{artist.name}}
			</td>
		</tr>
{% endfor %}
	</tbody>
</table>

{% endblock %}