{% extends('BooksmartMetricsSoundcloudBundle:Default:base.html.twig') %}

{% block body %}

{{form(form)}}

{{dump(results)}}
<table class = 'table table-striped'>
	<thead>
		<tr>
			<th>
				
			</th>
			<th>
				Name
			</th>
			<th>
				Location
			</th>
		</tr>
	</thead>
	<tbody>
{% for business in results.businesses %}
	<tr>
		<td>
			<img src = "{{business.image_url}}"></img>
		</td>
		<td>
			{{business.name}}
		</td>
		<td>
			{{business.location.address[0]}} {{business.location.city}}
		</td>
	</tr>
{% endfor %}
	</tbody>
</table>

{% endblock %}